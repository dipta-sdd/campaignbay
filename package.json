{
  "name": "campaignbay",
  "version": "1.0.0",
  "description": "CampaignBay for WooCommerce Product Discount WordPress Plugin.",
  "main": "index.js",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "build:legacy": "cross-env BUILD_TARGET=legacy wp-scripts build",
    "start:legacy": "cross-env BUILD_TARGET=legacy wp-scripts start",
    "makepot": "wp i18n make-pot . languages/campaignbay.pot --domain=campaignbay --slug=campaignbay --exclude='node_modules,vendor,src'",
    "make-json": "wp i18n make-json languages --no-purge && node helper/rename-translations.js"
  },
  "author": "sankarsan das",
  "license": "GPLv2 or later",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@wordpress/babel-plugin-makepot": "^6.26.0",
    "@wordpress/scripts": "^30.25.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "browser-sync": "^3.0.4",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.31.0",
    "@wordpress/dataviews": "^4.22.0",
    "@wordpress/date": "^5.28.0",
    "@wordpress/i18n": "^5.26.0",
    "@wordpress/icons": "^10.26.0",
    "@wordpress/react-i18n": "^4.26.0",
    "chart.js": "^4.5.0",
    "lucide-react": "^0.525.0",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.6.3"
  },
  "wp-i18n": {
    "domain": "campaignbay",
    "potFile": "languages/wpab-cb.pot"
  }
}
